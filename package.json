{
  "name": "easyrag",
  "version": "0.0.7",
  "description": "EasyRAG is the AI TypeScript library you wish you had when you got started.",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "test": "tsc && node -r tsconfig-paths/register ./dist/tests/index.js",
    "test:tools": "tsc && node -r tsconfig-paths/register ./dist/tests/tools.js",
    "test:conversation": "tsc && node -r tsconfig-paths/register ./dist/tests/conversation.js",
    "test:embedding": "tsc && node -r tsconfig-paths/register ./dist/tests/embedding.js",
    "test:ollama": "tsc && node -r tsconfig-paths/register ./dist/tests/ollama.js"
  },
  "keywords": [
    "rag",
    "openai",
    "typescript",
    "javascript",
    "chat",
    "ai",
    "llm",
    "agent",
    "tool"
  ],
  "author": "Alexander Mason",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "LICENSE"
  ],
  "dependencies": {
    "@ctrl/golang-template": "^1.4.1",
    "jsonschema": "^1.4.1"
  }
}
